{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "declaration": true,
    "outDir": "./lib",
    "downlevelIteration": true,
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "dom",
      "es5",
      "scripthost",
      "es2015.iterable",
      "ES2015.Collection"
    ]
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "tests"
  ]
}
